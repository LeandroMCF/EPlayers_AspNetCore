@model Equipe
@{
    ViewData["Title"] = "Equipes";
}

<form method="Post" action='@Url.Action("Cadastrar")'>
    <label>Id</label>
    <input type="text" name="IdEquipe"/>

    <label>Nome</label>
    <input type="text" name="Nome"/>

    <label>Imagens</label>
    <input type="text" name="Imagens"/>

    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-responsive">
    <thead>
        <th>Id</th>
        <th>Nome</th>
        <th>Imagens</th>
    </thead>
    <tbody>
        @foreach (Equipe equipe  in ViewBag.Equipes)
        {
            <tr>
                <td>@equipe.IdEquipe</td>
                <td>@equipe.Nome</td>
                <td>@equipe.Imagem</td>
            </tr>
        }
    </tbody>
</table>